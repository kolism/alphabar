{
  "manifest_version": 2,
  "name": "Pin tabs - Tab Manager",
  "short_name": "Keep tabs history",
  "version": "1.1.3",
  "description":
    "Save Tabs allows the user to pin tabs and keep track of them in the history for short periods.",
  "browser_action": {
    "default_title": "Show the list of pinned tabs",
    "default_popup": "pages/popup.html"
  },
  "author": "Pier Roberto Lucisano",
  "background": {
    "page": "pages/background.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["pages/index.js"],
      "run_at": "document_end",
      "exclude_matches": ["https://www.youtube.com/*"]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "storage",
    "alarms"
  ],
  "minimum_chrome_version": "60",
  "icons": {
    "16": "assets/list.png",
    "48": "assets/list.png",
    "128": "assets/list128.png"
  },
}